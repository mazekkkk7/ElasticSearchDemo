/**
 * mapping名称es_doctor 医生
 * image 图片
 * name 医生名称 
 * sex 医生性别
 * hospital 医院
 * hospital.name 医院名称
 * level 职称
 * level.code 职称代码
 * level.name 职称（主任医师、副主任医师）
 * level.order 职称顺序
 * dept 科室
 * dept.name 科室名称
 * dept.code 科室代码
 * area 地区
 * area.name 地区名称
 * tags 擅长
 * biography 个人简介
 * academicAchievements 学术成果
 * coordinates 经纬度(lat、lon)
 * score 患者评分
 * vcPrice 视频咨询服务价格
 * fcPrice 义诊咨询价格
 * imPrice 图文咨询价格
 * pmdPrice 私人医生服务价格
 * retPrice 远程急救服务价格
 * isExpert 是否专家
 * isVisit 是否开诊
 * isOpenVC 是否开启视频咨询
 * isOpenPMD 是否开启私人医生
 * isOpenRET 是否开启远程急救
 * isOpenFC 是否开通义诊
 * isOpenIM 是否开通图文
 * isOpenPhoneConsultation 是否开通电话咨询
 * isOpenFamilyDoctor 是否开通家庭医生
 * isReal 是否真实
 * canOpenVC 能否开通视频咨询
 * canOpenRET 能否开通远程急救
 * canOpenPMD 能否开通私人医生
 * fans 粉丝量
 * serviceCount 服务人次
 * orderCount 订单量
 * recommendPatient 患者推荐量
 * recommendDoctor 医生推荐量
 * assess 评价量
 * assessTags 评价标签
 * distance 距离（经纬度查询时使用）
 * todayLeftCount 今日剩余次数
 * fcTodayAllCount 医生总次数
 * rank 医生级别
 * rankCode 医生级别Code
 * createTime 创建时间
 * updateTime 更新时间
 */
POST /skykingkong_es_dev/es_doctor/_mapping
{
    "es_doctor": {
        "_all": {
            "analyzer": "ik_max_word",
            "search_analyzer": "ik_max_word",
            "term_vector": "no",
            "store": "false"
        },
        "properties": {
			"image": {
                "type": "text"
            },
            "name": {
                "type": "text",
                "analyzer": "ik_max_word",
                "search_analyzer": "ik_max_word",
                "include_in_all": "true"
            },
            "sex": {
                "type": "integer"
            },
            "hospital": {
                "properties": {
                    "name": {
                        "type": "text",
                        "analyzer": "ik_max_word",
                        "search_analyzer": "ik_max_word",
                        "include_in_all": "true"
                    }
                }
            },
            "level": {
                "properties": {
                    "code":{
                        "type":"text"
                    },
                    "name": {
                        "type": "text",
                        "analyzer": "ik_max_word",
                        "search_analyzer": "ik_max_word",
                        "include_in_all": "true"
                    },
                    "order": {
                        "type": "integer"
                    }
                    
                }
            },
            "dept": {
                "type":"nested",
                "properties": {
                    "name": {
                        "type": "text",
                        "analyzer": "ik_max_word",
                        "search_analyzer": "ik_max_word",
                        "include_in_all": "true"
                    },
                    "code":{
                        "type":"text",
                        "fielddata": true
                    }
                }
            },
			"area": {
                "properties": {
                    "name": {
                        "type": "text",
                        "analyzer": "ik_max_word",
                        "search_analyzer": "ik_max_word",
                        "include_in_all": "true"
                    }
                }
            },
            "tags": {
                "type": "text",
                "analyzer": "ik_max_word",
                "search_analyzer": "ik_max_word",
                "include_in_all": "true"
            },
			"biography": {
                "type": "text"
            },
			"academicAchievements": {
                "type": "text"
            },
            "coordinates": {
                "type": "geo_point"
            },
            "score": {
                "type": "double"
            },
            "vcPrice": {
                "type": "double"
            },
            "fcPrice": {
                "type": "double"
            },
            "pmdPrice": {
                "type": "double"
            },
            "retPrice": {
                "type": "double"
            },
    		"isExpert": {
                "type": "boolean"
            },
			"isVisit": {
                "type": "boolean"
            },
			"isOpenVC":{
				"type":"boolean"
			},
			"isOpenPMD":{
				"type":"boolean"
			},
			"isOpenRET":{
				"type":"boolean"
			},
            "isOpenFC": {
                "type":"boolean"
            },
            "isOpenIM":{
                "type":"boolean"
            },
            "isOpenPhoneConsultation":{
                "type":"boolean"
            },
            "isOpenFamilyDoctor":{
                "type":"boolean"
            },
            "isReal":{
    			"type":"boolean"
			},
            "canOpenVC":{
                "type":"boolean"
            },
            "canOpenRET":{
                "type":"boolean"
            },
            "canOpenPMD":{
                "type":"boolean"
            },
			"fans":{
				"type":"integer"
			},
			"serviceCount":{
				"type":"integer"
			},
            "orderCount":{
    			"type":"integer"
			},
			"recommendPatient":{
				"type":"integer"
			},
			"recommendDoctor":{
				"type":"integer"
			},
			"assess":{
				"type":"integer"
			},
			"assessTags": {
                "type": "text",
                "analyzer": "ik_max_word",
                "search_analyzer": "ik_max_word",
                "include_in_all": "true"
            },
			"distance":{
				"type":"double"
			},
            "todayLeftCount": {
                "type": "integer"
            },
            "fcTodayAllCount": {
                "type": "integer"
            },
            "rank": {
                "type": "integer"
            },
            "rankCode": {
                "type": "text"
            },
            "createTime":{
                "type": "date",
                "format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
            },
            "updateTime":{
                "type": "date",
                "format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
            }
        }
    }
}